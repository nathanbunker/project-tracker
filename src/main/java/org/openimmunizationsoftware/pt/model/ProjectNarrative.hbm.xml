<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"https://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.openimmunizationsoftware.pt.model">
  <class name="ProjectNarrative" table="project_narrative">
    <id name="narrativeId" type="int">
      <column name="narrative_id" />
      <generator class="native" />
    </id>
    <property name="projectId" type="int">
      <column name="project_id" not-null="true" />
    </property>
    <many-to-one name="project" column="project_id" class="Project" insert="false" update="false" lazy="proxy" fetch="select"/>
    <property name="contactId" type="int">
      <column name="contact_id" not-null="true" />
    </property>
    <many-to-one name="contact" column="contact_id" class="ProjectContact" insert="false" update="false" lazy="proxy" fetch="select"/>
    <property name="providerId" type="string">
      <column name="provider_id" length="30" not-null="true" />
    </property>
    <many-to-one name="provider" column="provider_id" class="ProjectProvider" insert="false" update="false" lazy="proxy" fetch="select"/>
    <property name="narrativeDate" type="timestamp">
      <column name="narrative_date" length="19" not-null="true" />
    </property>
    <property name="narrativeVerbString" type="string">
      <column name="narrative_verb" length="20" not-null="true" />
    </property>
    <property name="narrativeText" type="text">
      <column name="narrative_text" not-null="true" />
    </property>
  </class>
</hibernate-mapping>
