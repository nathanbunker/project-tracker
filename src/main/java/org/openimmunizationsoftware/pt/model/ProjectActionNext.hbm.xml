<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"https://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.openimmunizationsoftware.pt.model">
  <class name="ProjectActionNext" table="project_action_next">
    <id name="actionNextId" type="int">
      <column name="action_next_id" />
      <generator class="native" />
    </id>
    <property name="projectId" type="int">
      <column name="project_id" not-null="true" />
    </property>
    <many-to-one name="project" column="project_id" class="Project" insert="false" update="false" lazy="proxy" fetch="select"/>
    <property name="contactId" type="int">
      <column name="contact_id" not-null="true" />
    </property>
    <many-to-one name="contact" column="contact_id" class="ProjectContact" insert="false" update="false" lazy="proxy" fetch="select"/>
    <many-to-one name="provider" column="provider_id" class="ProjectProvider" lazy="proxy" fetch="select"/>
    <property name="nextActionStatusString" type="string">
      <column name="next_action_status" length="1" not-null="true" />
    </property>
    <property name="nextChangeDate" type="timestamp">
      <column name="next_change_date" length="19" />
    </property>
    <property name="nextDescription" type="string">
      <column name="next_description" length="1200" not-null="true" />
    </property>
    <property name="billable" type="yes_no">
      <column name="billable" length="1" not-null="true" />
    </property>
    <property name="nextDue" type="timestamp">
      <column name="next_due" length="19" />
    </property>
    <property name="nextActionType" type="string">
      <column name="next_action_type" length="16" />
    </property>
    <property name="nextTimeEstimate" type="java.lang.Integer">
      <column name="next_time_estimate" />
    </property>
    <property name="nextTimeActual" type="java.lang.Integer">
      <column name="next_time_actual" />
    </property>
    <property name="nextContactId" type="java.lang.Integer">
      <column name="next_contact_id" />
    </property>
    <many-to-one name="nextProjectContact" column="next_contact_id" class="ProjectContact" insert="false" update="false" lazy="proxy" fetch="select"/>
    <property name="priorityLevel" type="integer">
      <column name="priority_level" not-null="true" />
    </property>
    <property name="prioritySpecialString" type="string">
      <column name="priority_special" />
    </property>
    <property name="goalStatus" type="string">
      <column name="goal_status" length="1" />
    </property>
    <property name="templateActionNextId" type="java.lang.Integer">
      <column name="template_action_next_id" />
    </property>
    <property name="linkUrl" type="string">
      <column name="link_url" length="1200" />
    </property>
    <property name="nextNotes" type="string">
      <column name="next_notes" />
    </property>
    <property name="nextSummary" type="string">
      <column name="next_summary" />
    </property>
    <property name="nextFeedback" type="string">
      <column name="next_feedback" />
    </property>
    <property name="templateTypeString" type="string">
      <column name="template_type" />
    </property>
    <property name="nextDeadline" type="timestamp">
      <column name="next_deadline" length="19" />
    </property>
  </class>
</hibernate-mapping>
